{
	"name": "visual-debug",
	"displayName": "Visual Debugger",
	"description": "Visualize your debug sessions in a debug graph",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.45.0"
	},
	"categories": [
		"Debuggers"
	],
	"activationEvents": [
		"onDebug",
		"onCommand:visualDebug.open-visualization"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/boltlang/vscode-visual-debug-extension.git"
	},
	"license": "GPL-3.0",
	"main": "./dist/extension.min.js",
	"contributes": {
		"customEditors": [
			{
				"viewType": "visualDebug.debugGraph",
				"displayName": "Debug Graph",
				"selector": [
					{
						"filenamePattern": "*"
					}
				],
				"priority": "option"
			}
		],
		"commands": [
			{
				"command": "visualDebug.open-visualization",
				"title": "Run and Visualize"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "webpack --config webpack.prod.js",
		"lint": "eslint src --ext ts",
		"watch": "webpack --watch --config webpack.dev.js",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.2",
		"@types/mocha": "^7.0.2",
		"@types/node": "^12.x.x",
		"@types/vscode": "^1.45.1",
		"@typescript-eslint/eslint-plugin": "^3.1.0",
		"@typescript-eslint/parser": "^3.1.0",
		"eslint": "^7.2.0",
		"glob": "^7.1.6",
		"mocha": "^7.2.0",
		"ts-loader": "^7.0.5",
		"typescript": "^3.9.5",
		"uglifyjs-webpack-plugin": "^2.2.0",
		"vscode-test": "^1.4.0",
		"webpack": "^4.43.0",
		"webpack-cli": "^3.3.11",
		"webpack-merge": "^4.2.2"
	},
	"dependencies": {
		"vscode-debugprotocol": "^1.41.0"
	}
}
